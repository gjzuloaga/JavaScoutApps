<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" alignment="CENTER" spacing="10" fx:controller="files.javascoutapp.HelloController">
    <TextField fx:id="searchField" promptText="Enter player name" />
    <Button text="Search" onAction="#onSearchClick" />


    <Label text="Player ID:" />
    <Label fx:id="idLabel" />

    <Label text="Player API ID:" />
    <Label fx:id="playerApiIdLabel" />

    <Label text="Birthday:" />
    <Label fx:id="birthdayLabel" />

    <Label text="Height:" />
    <Label fx:id="heightLabel" />

    <Label text="Weight:" />
    <Label fx:id="weightLabel" />
</VBox>
